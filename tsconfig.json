{
    "compilerOptions": {
        "module": "commonjs",
        "outDir": "hanlders",
        "target": "ES2015",
        "allowJs": false,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "noImplicitAny": false,
        "skipLibCheck": true,
        "sourceMap": true,
        "strictNullChecks": false,
        "resolveJsonModule": true,
        "preserveConstEnums": true
    }
}
